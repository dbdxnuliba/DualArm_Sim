<?xml version="1.0"?>
<robot name = "dualarm" xmlns:xacro="http://www.ros.org/wiki/xacro" >

	<xacro:include filename="$(find dualarm_description)/urdf/link_to_joint.xacro" />
	<xacro:include filename="$(find dualarm_description)/urdf/robot_parameter.xacro" />
	<xacro:include filename="$(find dualarm_description)/urdf/dual_arm.gazebo.xacro" />

	<!-- Base parts -->
	<link name = "world"/>
	<joint name="base_joint" type="fixed">
		<origin
				xyz="0 0 0"
				rpy="0 0 0"/>
		<parent
				link="world"/>
		<child
				link="dual_arm_base_link"/>
	</joint>

	<link name="dual_arm_base_link"/>

	<joint name="dual_arm_base_joint" type="fixed">
		<origin
			xyz="0 0 0"
			rpy="0 0 0"/>
		<parent
			link="dual_arm_base_link"/>
		<child
			link="${base_link_name}"/>
	</joint>

	<link name="${base_link_name}">
		<inertial>
			<mass value="${base_link_mass}" />
			<origin
					rpy="0 0 0"
					xyz="0 0 0" />
			<inertia
					ixx="${base_link_ixx}"
					ixy="${base_link_ixy}"
					ixz="${base_link_ixz}"
					iyy="${base_link_iyy}"
					iyz="${base_link_iyz}"
					izz="${base_link_izz}" />
		</inertial>
		<visual>
			<origin
					rpy="0 0 ${M_PI}"
					xyz="0 0 0.3" />
			<geometry>
				<mesh
						filename="package://dualarm_description/meshes/dualarm/base.STL"
						scale="0.001 0.001 0.001"/>
			</geometry>
		</visual>
	</link>

	<!-- waist part -->
	<joint name="dualarm_joint1" type="revolute">
		<axis xyz="0 0 -1" />
		<limit
				effort="300"
